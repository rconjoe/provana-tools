{
  "configurations": {
    "run": {
      "adapter": "vscode-node",
      "breakpoints": {
        "exception": {
          "all": "N",
          "uncaught": "Y"
        }
      },
      "configuration": {
        "request": "launch",
        "protocol": "auto",
        "stopOnEntry": false,
        "console": "integratedTerminal",
        "program": "${script:${file\\}}",
        "cwd": "${workspaceRoot}"
      }
    }
  }
}
